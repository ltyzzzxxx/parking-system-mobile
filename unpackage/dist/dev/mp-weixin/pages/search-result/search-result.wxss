
.search {
	width: 100%;
	height: 80px;
	background-color: #fefefe;
}
.search-parking {
	position: absolute;
	top: 2.5%;
	right: 4%;
	left: 4%;
	border-radius: 50px;
	width: 92%;
	height: 6%;
	background-color: #f8f8fa;
}
.search-list {
	width: 100%;
	position: absolute;
	top: 80px;
	bottom: 0;
	left: 0;
	background-color: #f8f8fa;
}
.parking-item {
	margin: 30px auto 0 auto;
	background-color: #fefefe;
	height: 150px;
	border-radius: 10px;
	width: 80%;
	box-shadow: 10px 20px 20px -20px #eeeff5;
}
.no-carpark {
	position: absolute;
	top: 0%;
	height: 88%;
	width: 100%;
}

