
.search {
	width: 100%;
	height: 80px;
	background-color: #f6f6f6;
}
.search-parking {
	position: absolute;
	top: 2.5%;
	right: 4%;
	left: 4%;
	border-radius: 50px;
	width: 92%;
	height: 6%;
	background-color: #fefefe;
}
.list {
	display: flex;
	flex-wrap: wrap;
}
.history-item {
	margin: 0 5px 10px 10px;
	color: #8A8A8A;
	font-size: 28rpx;
	box-sizing: border-box;
	text-align: center;
	padding: 20rpx 20rpx;
	border-top: 2rpx solid #FFF;
	border-left: 2rpx solid #FFF;
	overflow: hidden;
	white-space: nowrap;
	text-overflow: ellipsis;
	background-color: #F7F7F7;
}

