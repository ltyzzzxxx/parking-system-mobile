
page {
	background-color: #f8f8fa;
}
.tabs {
	width: 100%;
	height: 45px;
	background-color: #fefefe;
	position: fixed;
	top: 0;
}
.search-list {
	width: 100%;
	display: flex;
	margin-top: 45px;
	flex-direction: column;
	background-color: #f8f8fa;
}
.coupon-item {
	margin: 20px auto 0px auto;
	background-color: #fefefe;
	height: auto;
	border-radius: 10px;
	width: 90%;
	padding: 15px 0 15px 0;
	box-shadow: 10px 20px 20px -20px #eeeff5;
}
.coupon-status {
	width: auto;
	height: 40rpx;
	background-color: #84cb86;
	border-radius: 3px;
	margin-right: 10px;
}
.status-blue {
	background-color: #007AFF;
}
.status-red {
	background-color: #DE5C5A;
}
.status-green {
	background-color: #84CB86;
}
.no-car {
	position: absolute;
	top: 8%;
	height: 88%;
	width: 100%;
}

