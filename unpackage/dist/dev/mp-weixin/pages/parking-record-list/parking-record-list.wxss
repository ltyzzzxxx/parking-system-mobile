
.tabs {
	width: 100%;
	height: 45px;
	background-color: #fefefe;
	position: fixed;
	top: 0;
}
.search-list {
	width: 100%;
	display: flex;
	margin-top: 45px;
	flex-direction: column;
	background-color: #f8f8fa;
}
.record-item {
	margin: 20px auto 0px auto;
	background-color: #fefefe;
	height: auto;
	border-radius: 10px;
	width: 90%;
	padding: 15px 0 15px 0;
	box-shadow: 10px 20px 20px -20px #eeeff5;
}
.record-status {
	width: auto;
	height: 40rpx;
	background-color: #84cb86;
	border-radius: 10px;
	margin-right: 15px;
}
.detail-button {
	width: auto;
	height: 50rpx;
	border-style: solid;
	border-width: 1px;
	border-color: #84cb86;
	border-radius: 30px;
}

